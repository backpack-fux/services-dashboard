            "to": "payments",
            "relationship": "provides"
        },
        {
            "from": "back-office",
            "to": "orders",
            "relationship": "provides"
        },
        {
            "from": "back-office",
            "to": "widgets",
            "relationship": "provides"
        },

        {
            "from": "card-services",
            "to": "card-issuance",
            "relationship": "provides"
        },
        {
            "from": "card-services",
            "to": "card-management",
            "relationship": "provides"
        },
        {
            "from": "card-services",
            "to": "transaction-viewing",
            "relationship": "provides"
        },
        {
            "from": "card-management",
            "to": "transaction-viewing",
