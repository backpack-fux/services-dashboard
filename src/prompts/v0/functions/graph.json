            "financial_challenges": ["follow_through", "resource_management"],
            "decision_style": {
                "speed_multiplier": 1.5,
                "risk_tolerance": "high",
                "waiting_period": "minimal",
                "confirmation_threshold": "low"
            },
            "optimal_capabilities": ["transfers", "card-issuance"],
            "requires": ["user-auth"]
        },
        "energy-type-generator": {
            "type": "experience",
            "description": "Generator financial interaction patterns",
            "traits": ["consistent", "methodical", "building"],
            "financial_strengths": ["steady_growth", "sustainable_patterns"],
            "financial_challenges": ["quick_decisions", "opportunity_cost"],
            "decision_style": {
                "speed_multiplier": 0.8,
                "risk_tolerance": "medium",
                "waiting_period": "response-based",
                "confirmation_threshold": "medium"
            },
            "optimal_capabilities": ["automated-savings", "recurring-transfers"],
